---
import Layout from "../layouts/Layout.astro";
import Title from "../components/Title.astro";
import Env from "../components/react/Env.jsx";

import Background from "../assets/wallpaper.webp";
---

<Layout>
    <Title />
    <div
        class="w-full min-h-screen pt-24 bg-cover bg-center flex flex-col items-center justify-center gap-12 pb-4"
        style={"background-image: url(" + Background.src + ");"}
    >
        <Env client:load />
    </div>
</Layout>
